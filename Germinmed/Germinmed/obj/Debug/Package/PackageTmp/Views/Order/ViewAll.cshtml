@model IEnumerable<Germinmed.Models.Order>
@{ 
    Layout = null;
}
<h4>Order List</h4>
<div class="box-body table-responsive no-padding">
    <table class="table table-hover" id="orderTable">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.UserName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ProducName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Qty)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreatedDate)
                </th>
                <th></th>
            </tr>
            </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.UserName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ProducName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Qty)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CreatedDate)
                    </td>
                    <td>
                        <a class="btn btn-danger btn-sm" onclick="Delete('@Url.Action("Delete","Order",new {id=item.Id})')"><i class="fa fa-trash fa-lg"></i></a>
                        <a class="btn btn-primary btn-sm" href="@Url.Action("Details","Order", new { id = item.Id })"><i class="fa fa-info fa-lg"></i></a>
                    </td>
                </tr>

            }
        </tbody>

    </table>
    </div>