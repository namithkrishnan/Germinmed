@model IEnumerable<Germinmed.Models.Brands>
@{ 
    Layout = null;
}
<div class="container">
    <div class="row">
        <div class="col-md-12 wow fadeInUp" data-wow-delay="1.2s">
            <div class="brands">
                <h3>Our <span>Brands</span> </h3>
                <div class="brand-images">
                    <div id="demos2">
                        <div class="owl-two owl-carousel owl-theme">

                            @foreach (var item in Model.SkipWhile(x => x.Id == 0 || x.ShowInBrandPage==false))
                            {
                                <div class="item"><img src="@Url.Content(item.ImageUrl)" style="height:93px; margin-bottom:40px;"> </div>
                                
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>