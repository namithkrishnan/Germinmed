@{
    if (Session["userID"] == null)
    {
        Response.Redirect("~/Admin/Index");
    }
}
    <!DOCTYPE html>
    <html>
    <head>
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>@ViewBag.Title - Germinmed Admin panel</title>
        <!-- Tell the browser to be responsive to screen width -->
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <!-- Google Font -->
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
        @*<link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />*@

        <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
        @Styles.Render("~/Content/admin-style")
        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body class="hold-transition skin-blue sidebar-mini">
        <div class="wrapper">
            <header class="main-header">
                <!-- Logo -->
                <a href="@Url.Action("Home", "Admin")" class="logo">


                    <!-- mini logo for sidebar mini 50x50 pixels -->
                    <span class="logo-mini"><b>Ge</b>Md</span>
                    <!-- logo for regular state and mobile devices -->
                    <span class="logo-lg"><b>Germinmed</b></span>
                </a>
                <!-- Header Navbar: style can be found in header.less -->
                <nav class="navbar navbar-static-top">

                    <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                        <span class="sr-only">Toggle navigation</span>
                    </a>

                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">

         <!-- User Account: style can be found in dropdown.less -->
          <li class="dropdown user user-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <img src="~/AppFiles/Images/DefaultUser.png" class="user-image" alt="User Image">
              <span class="hidden-xs">@Session["firstName"]</span>
            </a>
            <ul class="dropdown-menu">
                <!-- User image -->
                <li class="user-header">
                    <img src="~/AppFiles/Images/DefaultUser_bg.png" class="img-circle" alt="User Image">
                    <p>
                        @Session["firstName"] - Admin
                        <small>Member since @(Session["CreatedDate"])</small>
                    </p>
                </li>
                <!-- Menu Body -->
                @* <li class="user-body">
                <div class="row">
                   <div class="col-xs-4 text-center">
                     <a href="#">Followers</a>
                   </div>
                   <div class="col-xs-4 text-center">
                     <a href="#">Sales</a>
                   </div>
                   <div class="col-xs-4 text-center">
                     <a href="#">Friends</a>
                   </div>
                 </div>
                        <!-- /.row -->
        </li>*@
                <!-- Menu Footer-->
                <li class="user-footer">
                    <div class="pull-left">
                        <a href="@Url.Action("UserProfile", "Admin", new { id = Session["userID"] })" class="btn btn-default btn-flat">Profile</a>
                    </div>
                    <div class="pull-right">
                        <a href="@Url.Action("Logout", "Admin")" class="btn btn-default btn-flat">Sign out</a>
                    </div>
                </li>
            </ul>
          </li>


                            </ul>
                        </div>





                </nav>
            </header>
            <!-- Left side column. contains the logo and sidebar -->
            <aside class="main-sidebar">
                <!-- sidebar: style can be found in sidebar.less -->
                <section class="sidebar">
                    <!-- Sidebar user panel -->

                    <ul class="sidebar-menu">

                        <li></li>
                        @{ if (@Session["userTypeId"] != null && int.Parse(@Session["userTypeId"].ToString()) == 2)
                            {
                                <li><a href="@Url.Action("Index", "Category")"><i class="fa fa-cog"></i> <span>Category</span></a></li>
                                <li><a href="@Url.Action("Index", "Products")"><i class="fa fa-product-hunt"></i> <span>Products</span></a></li>
                                <li><a href="@Url.Action("Index", "Brand")"><i class="fa fa-registered"></i> <span>Brands</span></a></li>
                                <li><a href="@Url.Action("Index", "Client")"><i class="fa fa-users"></i> <span>Clients</span></a></li>
                                <li><a href="@Url.Action("Index", "Event")"><i class="fa fa-calendar-check-o"></i> <span>Events</span></a></li>

                                <li><a href="@Url.Action("Index", "Banner")"><i class="fa fa-tags"></i> <span>Banner</span></a></li>

                                <li><a href="@Url.Action("Index", "InnerBanner")"><i class="fa fa-tags"></i> <span>Inner Banner</span></a></li>

                                <li><a href="@Url.Action("Index", "Order")"><i class="fa fa-shopping-cart"></i> <span>Orders</span></a></li>

                                <li><a href="@Url.Action("Index", "User")"><i class="fa fa-user"></i> <span>Admin Users</span></a></li>

                                <li><a href="@Url.Action("RegisteredUsers", "User")"><i class="fa fa-user"></i> <span>Registered Users</span></a></li>

                                <li><a href="@Url.Action("Index", "Branch")"><i class="fa fa-user"></i> <span>Branches</span></a></li>

                                <li><a href="@Url.Action("Settings", "Admin", new { id = 1 })"><i class="fa fa-cogs"></i> <span>Settings</span></a></li>

                                <li><a href="@Url.Action("Index", "NewsLetter")"><i class="fa fa-envelope"></i> <span>NewsLetter</span></a></li>

                            }
                            else
                            {
                                <li><a href="@Url.Action("Index", "Order")"><i class="fa fa-shopping-cart"></i> <span>Orders</span></a></li>
                            }

                        }
                    </ul>


                </section>

                <!-- /.sidebar -->
            </aside>
            <!-- Content Wrapper. Contains page content -->

            <div class="content-wrapper">
                <section class="content-header">
                    <h1>
                        @ViewContext.RouteData.Values["Controller"].ToString()
                        <small></small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="@Url.Action("Home","Admin")"><i class="fa fa-dashboard"></i> Home</a></li>
                        <li class="active"><a href="@Url.Action(ViewContext.RouteData.Values["Action"].ToString(),ViewContext.RouteData.Values["Controller"].ToString())">@ViewContext.RouteData.Values["Controller"].ToString()</a></liclass="active">
                       
                    </ol>
                </section>

                <section class="content">
                    <div class="box">
                        <div class="box-header with-border">
                            <h3 class="box-title">Admin Panel</h3>
                            <div class="box-tools pull-right">
                                <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                                        title="Collapse">
                                    <i class="fa fa-minus"></i>
                                </button>
                                <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                    <i class="fa fa-times"></i>
                                </button>
                            </div>
                        </div>

                        @RenderBody()
                        <div class="box-footer">
                          
                        </div>
                        <!-- /.box-footer-->
                    </div>
</section>
            </div>
            <!-- /.content-wrapper -->
            <footer class="main-footer">
                <div class="pull-right hidden-xs">
                    <b>Version</b> 2.4.0
                </div>
                <strong>&copy; @DateTime.Now.Year - GerminMed. Doha, Qatar <a href="#"></a>.</strong>

            </footer>

        </div>

        <div class="loaderbody" id="loaderbody">
            <div class="loader"></div>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
        <script src="~/Scripts/Scripts.js"></script>

        <script src="~/Scripts/PushMenu.js"></script>
        <script src="~/Scripts/notify.min.js"></script>
    </body>
</html>
